    "koa-static": "^4.0.2",
    "koa-views": "^6.1.2",
    "mario-architecture-components": "0.0.19",
    "mario-ducks": "0.0.86",
    "mario-functional": "0.0.5",
    "mario-pure": "0.0.9",
    "mario-utilities": "0.0.40",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "rxjs": "^5.5.2"
  },
