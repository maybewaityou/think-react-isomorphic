{
  "name": "think-react-isomorphic",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "watch": "tsc -w",
    "start": "nodemon --watch 'server/**/*' -e ts,tsx --exec 'ts-node' ./server/index.ts",
    "clear": "rm -rf dist && rm -rf dist:dll && rm -rf .uglify-cache && rm -rf .uglify-dll-cache && rm -rf stats.json",
    "build": "webpack --config config/webpack.dev.config.js",
    "build:dll": "webpack --config config/webpack.dll.config.js",
    "build:dev": "webpack --config config/webpack.dev.config.js --profile --json > stats.json",
    "build:pro": "webpack --config config/webpack.pro.config.js",
    "analyzer": "webpack-bundle-analyzer stats.json",
    "lint": "tslint -c tslint.json 'src/**/*.*'"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maybewaityou/think-react-isomorphic.git"
  },
  "keywords": [
    "koa",
    "think"
  ],
  "author": "MeePwn",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/maybewaityou/think-react-isomorphic/issues"
  },
  "homepage": "https://github.com/maybewaityou/think-react-isomorphic#readme",
  "dependencies": {
    "ejs": "^2.5.7",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.2.0",
